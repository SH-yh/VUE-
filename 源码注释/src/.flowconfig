[ignore]
.*/node_modules/.*
.*/test/.*
.*/scripts/.*
.*/examples/.*
.*/benchmarks/.*
[include]

[libs]

[lints]

[options]
module.name_mapper='^compiler/\(.*\)$' -> '<PROJECT_ROOT>/compiler/\1'
module.name_mapper='^core/\(.*\)$' -> '<PROJECT_ROOT>/core/\1'
module.name_mapper='^shared/\(.*\)$' -> '<PROJECT_ROOT>/shared/\1'
module.name_mapper='^web/\(.*\)$' -> '<PROJECT_ROOT>/platforms/web/\1'
module.name_mapper='^weex/\(.*\)$' -> '<PROJECT_ROOT>/platforms/weex/\1'
module.name_mapper='^server/\(.*\)$' -> '<PROJECT_ROOT>/server/\1'
module.name_mapper='^entries/\(.*\)$' -> '<PROJECT_ROOT>/entries/\1'
module.name_mapper='^sfc/\(.*\)$' -> '<PROJECT_ROOT>/sfc/\1'
suppress_comment= \\(.\\|\n\\)*\\$flow-disable-line
[strict]
